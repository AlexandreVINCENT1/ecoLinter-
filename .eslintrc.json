{
    "env": {
        "browser": true,
        "es6": true,
		"mocha": true
    },
    "extends": "eslint:recommended",
	"parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module",
		"requireConfigFile": false
		
    },
	"plugins": ["greenit"],
    "rules": {
        "quotes": ["warn", "single"]
    }
}